import java.io.*;
public class file_i_o 
{
    public static void main(String[] args)
    {
		try 
		{
			FileWriter f=new filewriter("d:\\java\\javawrok\\newfolder");
			try
			{
				f.write("my name is mansi...!");
				f.write("i am student ...!");
			}
			finally
			{
				f.close();
			}
			System.out.println("Succedfully Data Wrote in File");
		}
		catch(IOException i)
		{
			System.out.println(i);
		}
    }
}
