package file_io;

import java.io.*;
class FileWriter
{
    public static void main(String[] args)
    {
    	   try 
    		{
    			FileWriter F=new FileWriter("d:\\java\\javawrok\\newfolder")
    			try
    			{
    				F.write("my name is mansi....!");
    			}
    			finally
    			{
    				F.close();
    			}
    			System.out.println("Succedfully Data Wrote in File");
    		}
    		catch(IOException i)
    		{
    			System.out.println(i);
    		}
    }
}
