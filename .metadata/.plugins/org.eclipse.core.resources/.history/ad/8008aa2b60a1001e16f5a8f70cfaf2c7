package file_io;

import java.io.File;
import java.io.IOException;

public class FileWriter
{
     public static void main(String[] args)
     {
		try
		{
			FileWriter F=new FileWriter("C:\\Users\\Dell\\Desktop\\LC.txt");
			try
			{
				F.Write("MY NAME IS MANSI...!");
			}
			finally
			{
				F.close();
			}
			System.out.println("succesfully data wrote in file");
		}
    	catch(IOException i)
    	{
    		System.out.println();
    	}
    	 
    	 
	}
}
